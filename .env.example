# Auto-Deployer Environment Variables
# Copy this file to .env and fill in your values
# Or set these as system environment variables

# ===================
# LLM Configuration
# ===================

# API Key for LLM provider (required)
# For Gemini: AUTO_DEPLOYER_GEMINI_API_KEY
# Generic: AUTO_DEPLOYER_LLM_API_KEY
AUTO_DEPLOYER_GEMINI_API_KEY=your-gemini-api-key-here

# HTTP Proxy for LLM requests (optional)
# Example: http://127.0.0.1:7890
AUTO_DEPLOYER_LLM_PROXY=

# ===================
# SSH Configuration
# ===================

# Default SSH host (required for deployment)
AUTO_DEPLOYER_SSH_HOST=192.168.1.100

# SSH port (default: 22)
AUTO_DEPLOYER_SSH_PORT=22

# SSH username (required)
AUTO_DEPLOYER_SSH_USERNAME=your-username

# SSH password (for password authentication)
AUTO_DEPLOYER_SSH_PASSWORD=your-password

# SSH private key path (for key authentication)
# If set, key authentication will be used instead of password
AUTO_DEPLOYER_SSH_KEY_PATH=
